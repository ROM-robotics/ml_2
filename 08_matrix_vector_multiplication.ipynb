    "import numpy as np\n",
    "\n",
    "v = np.array([-1,2])\n",
    "\n",
    "ij = np.array([ [3,1],[1,2] ])\n",
    "\n",
    "v_t = np.matmul(ij, v)\n",
    "\n",
    "print(\"\\nMatrix ij:\",ij, \"\\nVector v:\", v, \"\\nTransformed vector v_t:\",v_t, sep=\"\\n\")\n"
